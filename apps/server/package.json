{
  "name": "@scheduleright/server",
  "version": "0.0.1",
  "description": "SheduleRight Node API Server",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed": "tsx src/seeds/seed.ts",
    "db:indexes": "tsx src/db/indexes.ts",
    "db:indexes:list": "tsx src/db/indexes.ts list",
    "db:mysql:init": "tsx src/db/mysql-init.ts",
    "db:mysql:migrate": "tsx src/db/mysql/migrate.ts",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "fastify": "^4.25.2",
    "@fastify/cors": "^8.5.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "zod": "^3.22.4",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.3",
    "nano": "^10.1.0",
    "mysql2": "^3.11.3",
    "bullmq": "^5.3.2",
    "redis": "^4.6.12",
    "twilio": "^4.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/jsonwebtoken": "^9.0.7",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "vitest": "^1.1.0"
  }
}
